{
  "private": true,
  "name": "personal-site",
  "description": "",
  "author": "Ben Crevis",
  "license": "MIT",
  "scripts": {
    "build": "run build:css && run build:remix",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run dev:css & run dev:remix",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "test": "vitest",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "start-local": "node -r dotenv/config node_modules/.bin/remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "1.3.5",
    "@remix-run/react": "^1.3.5",
    "@remix-run/serve": "^1.3.5",
    "@remix-run/vercel": "^1.3.5",
    "@vercel/node": "^1.14.0",
    "chart.js": "^3.7.1",
    "chartjs-adapter-moment": "^1.0.0",
    "dotenv": "^16.0.0",
    "firebase": "^9.6.10",
    "firebase-admin": "^10.0.2",
    "leaflet": "^1.7.1",
    "moment": "^2.29.2",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.27.0",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-bricks": "^3.2.22",
    "react-bricks-ui": "^0.3.20",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^18.0.0",
    "remix": "^1.3.5"
  },
  "devDependencies": {
    "@remix-run/dev": "v0.0.0-experimental-9aba8b51",
    "@remix-run/eslint-config": "^1.3.5",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^14.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.4",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
