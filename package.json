{
  "private": true,
  "name": "personal-site",
  "description": "",
  "author": "Ben Crevis",
  "license": "MIT",
  "scripts": {
    "build": "run build:css && run build:remix",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run dev:css & run dev:remix",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode --require ./mocks -- @remix-run/dev:remix dev",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "test": "vitest",
    "start": "cross-env NODE_ENV=production remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "start-local": "node -r dotenv/config node_modules/.bin/remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@remix-run/vercel": "^1.5.1",
    "@vercel/node": "^2.1.0",
    "chart.js": "^3.8.0",
    "chartjs-adapter-moment": "^1.0.0",
    "dotenv": "^16.0.1",
    "firebase": "^9.8.3",
    "firebase-admin": "^10.3.0",
    "leaflet": "^1.8.0",
    "moment": "^2.29.3",
    "prism-theme-night-owl": "^1.4.0",
    "prismjs": "^1.28.0",
    "prop-types": "^15.8.1",
    "react": "^18.1.0",
    "react-bricks": "^3.2.26",
    "react-bricks-ui": "^0.4.1",
    "react-chartjs-2": "^4.2.0",
    "react-dom": "^18.1.0",
    "react-slick": "^0.29.0",
    "remix": "^1.5.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.3",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^5.2.0",
    "msw": "^0.42.1",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "tailwindcss": "^3.1.2",
    "ts-node": "^10.8.1",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-tsconfig-paths": "^3.5.0",
    "vitest": "^0.14.2"
  },
  "engines": {
    "node": "=16.x"
  },
